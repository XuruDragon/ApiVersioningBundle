{
  "name": "xurudragon/api-versioning-bundle",
  "type": "symfony-bundle",
  "description": "Symfony APIVersioningBundle",
  "keywords": ["versioning", "api"],
  "license": "MIT",
  "homepage": "https://github.com/XuruDragon/ApiVersioningBundle",
  "authors": [
    {
      "name": "XuruDragon"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "symfony/http-foundation": ">=2.7"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.2.20 || ^2.13@dev",
    "phpunit/phpunit": "^4.8.36 || ^5.7.27 || ^6.5.12 || ^7.3.2",
    "symfony/phpunit-bridge": "*"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "XuruDragon\\ApiVersioningBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "XuruDragon\\ApiVersioningBundle\\Tests\\": "/Tests/"
    }
  },
  "minimum-stability": "dev"
}
